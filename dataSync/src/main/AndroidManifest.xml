<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application>


        <service
            android:name="com.scanner.datasync.Services.DataSyncService"
            android:directBootAware="true"
            android:enabled="true"
            android:exported="true"
            android:label="The service created workmanager async"
            android:foregroundServiceType="connectedDevice|location|dataSync"
            android:permission="android.permission.BIND_JOB_SERVICE"
            android:showOnLockScreen="true"
            android:stopWithTask="false"
            tools:ignore="MissingClass,WrongManifestParent" >
            <intent-filter>
                <action android:name="dataSync1" />
                <action android:name="dataSync2" />
            </intent-filter>

        </service>

    </application>

</manifest>