<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >
    <application>
  <!--      //TODO  start application library-->
        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="${applicationId}.androidx-startup"
            android:enabled="true"
            android:exported="false"
            tools:ignore="WrongManifestParent"
            tools:node="merge" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="${applicationId}.workmanager-init"
            android:enabled="true"
            android:exported="false"
            tools:ignore="WrongManifestParent"
            tools:node="merge" />






        <service
            android:name=".businesslayer.Service.DataSyncService"
            android:directBootAware="true"
            android:enabled="true"
            android:exported="true"
            android:foregroundServiceType="connectedDevice|location|dataSync"
            android:permission="android.permission.BIND_JOB_SERVICE"
            android:showOnLockScreen="true"
            android:stopWithTask="false"
            tools:ignore="WrongManifestParent" >
            <intent-filter>
                <action android:name="dataSync1" />
                <action android:name="dataSync2" />
            </intent-filter>

        </service>






     <!--   //TODO  end appication library-->
    </application>

</manifest>